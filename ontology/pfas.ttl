###  http://purl.obolibrary.org/obo/RO_0001015
obo:RO_0001015 rdf:type owl:ObjectProperty ;
               owl:inverseOf obo:RO_0001025 ;
               rdfs:domain schema:Place ;
               rdfs:range schema:Organization ;
               rdfs:label "location of" ;
               skos:definition "A relation between two independent continuants, the target and the location, in which the target is entirely within the location." .


###  http://purl.obolibrary.org/obo/RO_0001025
obo:RO_0001025 rdf:type owl:ObjectProperty ;
